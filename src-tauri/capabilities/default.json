{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default Permissions",
  "windows": ["main"],
  "permissions": [
    "log:default",
    "app:default",
    "resources:default",
    "fs:default",
    "menu:default",
    "path:default",
    "tray:default",
    "event:default",
    "window:default",
    "window:allow-minimize",
    "window:allow-maximize",
    "window:allow-unmaximize",
    "window:allow-close",
    "window:allow-start-dragging",
    "image:default",
    "notification:default",
    "os:allow-platform",
    "dialog:allow-open",
    "dialog:allow-ask",
    "dialog:allow-save",
    "dialog:allow-confirm",
    "dialog:allow-message",
    "process:allow-exit",
